{
  "name": "proskomma-core",
  "version": "0.10.4",
  "description": "A Scripture Runtime Engine",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/module.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "rm -fr dist && vite build",
    "test": "export PKSRC=dist && bash -c \"tape -r @babel/register test/code/**/*.cjs | node_modules/faucet/bin/cmd.js\"",
    "win:test": "set PKSRC=dist&& bash -c \"tape -r @babel/register test/code/**/*.cjs | node_modules/faucet/bin/cmd.js\"",
    "rawTest": "export PKSRC=dist && bash -c \"tape -r @babel/register test/code/*.cjs\"",
    "oneTest": "bash -c \"tape -r @babel/register test/code/$TESTSCRIPT.cjs\"",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Proskomma/proskomma-core.git"
  },
  "keywords": [
    "USFM",
    "USX",
    "Scripture",
    "parser",
    "lexer",
    "Proskomma"
  ],
  "author": "Mark Howe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Proskomma/proskomma-core/issues"
  },
  "homepage": "https://github.com/Proskomma/proskomma-core#readme",
  "dependencies": {
    "@babel/preset-env": "^7.23.9",
    "@graphql-tools/schema": "^10.0.2",
    "async-mutex": "^0.4.1",
    "base-64": "^1.0.0",
    "base64-js": "^1.5.1",
    "bitset": "^5.1.1",
    "buffer": "^6.0.3",
    "deep-copy-all": "^1.3.4",
    "deep-equal": "^2.2.3",
    "easy-crc32": "^0.0.2",
    "fs-extra": "^11.2.0",
    "graphql": "^v16.8.1",
    "proskomma-json-tools": "^0.6.5",
    "pure-uuid": "^1.8.1",
    "sax": "^1.3.0",
    "stream": "0.0.2",
    "string_decoder": "^1.3.0",
    "utf8-string-bytes": "^1.0.3",
    "util": "^0.12.5",
    "xregexp": "^5.1.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.23.10",
    "@babel/register": "^7.23.7",
    "events": "^3.3.0",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "faucet": "^0.0.4",
    "tape": "^5.7.4",
    "typescript": "^5.3.3",
    "vite": "^5.0.12"
  }
}
